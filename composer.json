{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "dyrynda/laravel-model-uuid": "^6.6",
        "firebase/php-jwt": "^6.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.75",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "vyuldashev/laravel-openapi": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "db:fresh": [
            "@php artisan migrate:fresh --seed",
            "@generate-ide-helpers"
        ],
        "generate-ide-helpers": [
            "@php artisan ide-helper:generate -WHn",
            "@php artisan ide-helper:meta -n",
            "@php artisan ide-helper:model -Mrpn"
        ],
        "setup": [
            "chmod 775 ./setup.sh",
            "./setup.sh -cld"
        ]
    },
    "scripts-descriptions": {
        "db:fresh": "Truncates, migrates and seeds the database.",
        "generate-ide-helpers": "Generates IDE helper files.",
        "setup": "Used for initial setup on a server."
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
